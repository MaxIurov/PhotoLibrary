@model Nullable<DateTime>
@{
    string strdt = "";
    if (Model != null)
    {
        strdt = Model.GetValueOrDefault(DateTime.Now).ToShortDateString();
    }
    @Html.TextBox("", strdt, new { @class = "date-picker", type = "text" })
    @*@Html.TextBox("", dt, new { @class = "date-picker", type = "text" });*@
}